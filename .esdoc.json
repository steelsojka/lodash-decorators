{
  "source": "./buildDocs",
  "destination": "./docs",
  "plugins": [{
    "name": "esdoc-standard-plugin",
    "option": {
      "accessor": {"access": [ "public" ]},
      "test": {
        "type": "mocha",
        "source": "./buildDocs",
        "includes": [ ".*\\.spec\\.js" ]
      },
      "brand": {"title": "Lodash Decorators Documentation"}
    }
  }],
  "excludes": [ ".*\\.spec\\.js" ]
}